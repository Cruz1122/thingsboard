# ThingsBoard Docker configuration

# Database type: postgres or hybrid
DATABASE=postgres

# Cache type: valkey, valkey-cluster, or valkey-sentinel  
CACHE=valkey

# Queue type: kafka or confluent
TB_QUEUE_TYPE=kafka

# Docker repository and version
DOCKER_REPO=thingsboard
TB_VERSION=3.8.0
TB_NODE_DOCKER_NAME=tb-node
JS_EXECUTOR_DOCKER_NAME=tb-js-executor

# Transport docker names
HTTP_TRANSPORT_DOCKER_NAME=tb-http-transport
MQTT_TRANSPORT_DOCKER_NAME=tb-mqtt-transport
COAP_TRANSPORT_DOCKER_NAME=tb-coap-transport
LWM2M_TRANSPORT_DOCKER_NAME=tb-lwm2m-transport
SNMP_TRANSPORT_DOCKER_NAME=tb-snmp-transport

# Other services
WEB_UI_DOCKER_NAME=tb-web-ui
TB_VC_EXECUTOR_DOCKER_NAME=tb-vc-executor
LOAD_BALANCER_NAME=haproxy

# Enable monitoring (Prometheus/Grafana)
MONITORING_ENABLED=false

# Enable EDQS (Event Driven Query Service)
EDQS_ENABLED=false

# Java options
JAVA_OPTS=

# ThingsBoard configuration
TB_HOST=localhost
TB_PORT=8080
